name: "Create PR"
on:
  push:
    branches:
      - test
      
jobs: 
  pull-request-for-sub branch:
      runs-on: ubuntu-latest
      steps:       
      - name: run -io
        id: pr
        uses: emiliopedrollo/create-pull-request@v1.1.0
        with :
          base: io
          head: ${{ github.ref_name }}
          title: ${{ github.event.head_commit.id }}
          body: ${{ github.event.head_commit.message }}
          milestone: 1
          labels: patching
      - name: run -internal
        id: pr
        uses: emiliopedrollo/create-pull-request@v1.1.0
        with :
          base: internal
          head: ${{ github.ref_name }}
          title: ${{ github.event.head_commit.id }}
          body: ${{ github.event.head_commit.message }}
          milestone: 1
          labels: patching
